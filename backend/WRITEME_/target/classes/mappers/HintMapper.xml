<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.WRITEME.dao.HintDAO">

    <select id="selectHint" parameterType="com.WRITEME.model.HintDTO" resultType="com.WRITEME.model.HintDTO">
        SELECT `keywordID`, `hintID`, `hintName`
          FROM keywordHint
         WHERE `keywordID` = #{keywordID}

    </select>



</mapper>

